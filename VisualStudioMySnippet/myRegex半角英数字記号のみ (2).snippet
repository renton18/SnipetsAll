<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets
    xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>空白行を削除する</Title>
      <Shortcut>myRegex</Shortcut>
    </Header>
    <Snippet>
      <Code Language="cSharp">
        <![CDATA[Regex.Replace(str	, @"^[\r\n]+", string.Empty, RegexOptions.Multiline );]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
